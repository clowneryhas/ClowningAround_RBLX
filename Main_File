local function SWPOPGM_fake_script() -- Admin_Button_1.Handler 
	local script = Instance.new('LocalScript', Admin_Button_1)

	local Button = script.Parent
	local StarterGUI = game:GetService("StarterGui")
	
	Button.MouseButton1Click:Connect(function()
		StarterGUI:SetCore("SendNotification", {
			Title = "System Message",
			Text = "Loading CMD-x"
		})
	end)
end
coroutine.wrap(SWPOPGM_fake_script)()
local function QIRL_fake_script() -- Admin_Button_5_2.Button_Handler 
	local script = Instance.new('LocalScript', Admin_Button_5_2)

	local Button= script.Parent
	Button.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Vynixius/main/Prison%20Life/Script.lua"))()
	end)
end
coroutine.wrap(QIRL_fake_script)()
local function GDUZPYX_fake_script() -- Avatar_Icon.LocalScript 
	local script = Instance.new('LocalScript', Avatar_Icon)

	----//// Thanks for reading it, nothing special loser <3
	----///// Take an L not a W....
	
	local Avatar_Holder = script.Parent
	local Player = game.Players.LocalPlayer
	local Players = game.Players
	
	Avatar_Holder.Image = Players:GetUserThumbnailAsync(Player.UserId,Enum.ThumbnailType.AvatarBust,Enum.ThumbnailSize.Size420x420)
end
coroutine.wrap(GDUZPYX_fake_script)()
local function KUWZCZ_fake_script() -- Main_Holder.Main_Handler 
	local script = Instance.new('LocalScript', Main_Holder)

	UIS = game:GetService("UserInputService")
	local StarterGUI = game:GetService("StarterGui")
	local Player = game.Players.LocalPlayer
	local DraggableFrame = script.Parent
	DraggableFrame.Active = true
	DraggableFrame.Draggable = true
	local PosClosed = UDim2.new(-0.45, 0, 0.286, 0)--//////// Closed Tween Position  \\\\\\\\-
	local PosOpened = UDim2.new(0.019, 0, 0.286, 0) --//////// Opened Tween Position  \\\\\\\\-
	
	local Opened = false
	UIS.InputBegan:Connect(function(KeyCode)
		if KeyCode.KeyCode == Enum.KeyCode.RightAlt then
			if Opened then
				DraggableFrame:TweenPosition((PosClosed), "Out", "Linear")
				Opened = false
			else
				DraggableFrame:TweenPosition((PosOpened), "In", "Linear")
				Opened = true
			end
		end
	end)
	
	local Button_Close = script.Parent.Close_Button
	
	Button_Close.MouseButton1Click:Connect(function()
		DraggableFrame:TweenPosition((PosClosed), "Out", "Linear")
		Opened = false
	end)
	
	StarterGUI:SetCore("SendNotification", {
		Title = "System Message",
		Text = "Thanks for using Clowning Around!"
	})
	
	local Welcome_User = script.Parent.Welcome_Label
	Welcome_User.Text = "Welcome, " ..Player.Name
end
coroutine.wrap(KUWZCZ_fake_script)()
