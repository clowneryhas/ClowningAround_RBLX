-- Scripts:

local function QQDB_fake_script() -- ABOUT_BUTTON.About_Handler 
	local script = Instance.new('LocalScript', ABOUT_BUTTON)

	local button = script.Parent
	local About_Page = script.Parent.Parent.Parent.Background_Holder.ABOUT_Page
	local Scripts_Page = script.Parent.Parent.Parent.Background_Holder.SCRIPTS_Page
	local Admin_Page = script.Parent.Parent.Parent.Background_Holder.ADMIN_Page
	button.MouseButton1Click:Connect(function()
		About_Page.Active = true
		About_Page.Visible = true
		Scripts_Page.Active = false
		Scripts_Page.Visible = false
		Admin_Page.Visible = false
		Admin_Page.Active = false ----///// Means disappears :))))
	end)
end
coroutine.wrap(QQDB_fake_script)()
